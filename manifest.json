{
  "manifest_version": 2,
  "name": "DuckDuckGo Tracker Blocker",
  "version": "0.0.1",
  "background": {
    "scripts": ["background.js"]
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["main.js"]
    }
  ],
  "permissions": ["webRequest", "webRequestBlocking", "<all_urls>"]
}
